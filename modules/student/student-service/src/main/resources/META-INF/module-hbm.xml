<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.ecw.student.model.Student" />
	<class name="com.ecw.student.model.impl.StudentImpl" table="Student">
		<id name="Id" type="long">
			<generator class="assigned" />
		</id>
		<property name="Name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="Address" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="Std" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="Divison" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>