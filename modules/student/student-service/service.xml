<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="com.ecw.student">
	<namespace>ecw</namespace>
    <entity name="Student" local-service="true" table="Student">
        <column name="Id" type="long" primary="true"></column>
        <column name="Name" type="String"></column>
        <column name="Address" type="String"></column>
        <column name="Std" type="int"></column>
        <column name="Divison" type="String"></column>
        <finder name="StdAndDiv" return-type="Collection">
            <finder-column name="Std"></finder-column>
            <finder-column name="Divison"></finder-column>
        </finder>
    </entity>	
</service-builder>